{
  "manifest_version": 3,
  "name": "Material NewTab",
  "version": "1.0",
  "description": "NewTab replacement for all chromium browsers",
  "permissions": [
    "bookmarks","storage", "activeTab", "scripting"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "Icons/icon16.png",
    "48": "Icons/icon48.png",
    "128": "Icons/icon128.png"
  }
}